  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development&& astro dev",
    "dev2": "cross-env NODE_ENV=development&& astro dev --port 3001",
    "start": "cross-env NODE_ENV=development&& astro dev",
    "build": "cross-env NODE_ENV=production&& astro build",
    "preview": "astro preview",
    "astro": "astro",
    "server": "node ./dist/server/entry.mjs",
    "migrate": "prisma migrate dev && kysely-codegen",
    "diff": "yarn prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > diff.sql",
    "push": "prisma db push && kysely-codegen",
    "gen": "prisma generate && kysely-codegen",
    "introspect": "prisma db pull && prisma generate && kysely-codegen --dialect mysql",
    "postinstall": "kysely-codegen --dialect mysql",
    "email-dev": "email dev --port 3006",
    "email-export": "email export"
  },
